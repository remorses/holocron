{
  "name": "website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm generate-example-docs-json && NODE_OPTIONS=--max-old-space-size=6144 NODE_ENV=production doppler run -c production --preserve-env -- react-router build",
    "build-vercel": "pnpm bootstrap && pnpm generate-example-docs-json && pnpm react-router typegen && pnpm tsc && NODE_OPTIONS=--max-old-space-size=6144 NODE_ENV=production react-router build",
    "dev": "doppler run -- react-router dev --port 7664",
    "dev:prod": "doppler run -c production -- react-router dev --port 7664",
    "fly": "fly --app fumabase-website-prod",
    "generate-example-docs-json": "pnpm vite-node scripts/generate-example-docs-json.ts",
    "test": "FORCE_COLOR=true  doppler run --preserve-env -- pnpm vitest",
    "framer": "pnpm unframer 6ea1ba37e05fac32 --outDir src/framer --external --jsx",
    "framer-orin": "pnpm unframer dfa04aa1c5bdec07 --outDir src/framer-orin --external --jsx",
    "deployment": "doppler run -c production -- pnpm vite-node scripts/deployment.ts",
    "_dev_cloudflare": "doppler secrets download --format=env --no-file > .dev.vars && react-router dev --port 7664",
    "gen-client": "DIRS=(../cli/src/generated ../docs-website/src/generated) && tsc --incremental; for DIR in \"${DIRS[@]}\"; do rm -rf $DIR && mkdir -p $DIR && cp dist/src/lib/spiceflow-client.* $DIR; done",
    "start": "doppler run -- node ./build/server/index.js --port 7664",
    "typecheck": "react-router typegen && tsc"
  },
  "exports": {
    "./*": "./*"
  },
